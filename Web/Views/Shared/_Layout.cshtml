<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="//kendo.cdn.telerik.com/2022.3.1109/styles/kendo.common.min.css" />
    <link rel="stylesheet" href="//kendo.cdn.telerik.com/2022.3.1109/styles/kendo.default.min.css" />
    <link rel="stylesheet" href="//kendo.cdn.telerik.com/2022.3.1109/styles/kendo.mobile.all.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
</head>
<body>
    @RenderBody()

    <script src="//kendo.cdn.telerik.com/2022.3.1109/js/jquery.min.js"></script>
    <script src="//kendo.cdn.telerik.com/2022.3.1109/js/kendo.all.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
